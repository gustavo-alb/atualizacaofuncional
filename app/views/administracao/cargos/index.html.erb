<p id="notice"><%= notice %></p>

<h1>Listing Administracao_cargos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Codigo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administracao_cargos.each do |administracao_cargo| %>
      <tr>
        <td><%= administracao_cargo.nome %></td>
        <td><%= administracao_cargo.codigo %></td>
        <td><%= link_to 'Show', administracao_cargo %></td>
        <td><%= link_to 'Edit', edit_administracao_cargo_path(administracao_cargo) %></td>
        <td><%= link_to 'Destroy', administracao_cargo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cargo', new_administracao_cargo_path %>
